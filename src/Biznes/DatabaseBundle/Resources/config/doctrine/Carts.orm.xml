<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Biznes\DatabaseBundle\Entity\Carts" table="carts">
    <indexes>
      <index name="carts_id_user_fk1" columns="id_order"/>
      <index name="carts_id_state_fk1" columns="id_product"/>
      <index name="carts_id_payment_fk1" columns="id_realization_method"/>
    </indexes>
    <id name="idCart" type="integer" column="id_cart">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idOrder" type="integer" column="id_order" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <many-to-one field="idRealizationMethod" target-entity="RealizationMethods" fetch="LAZY">
      <join-columns>
        <join-column name="id_realization_method" referenced-column-name="id_realization_method"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProduct" target-entity="Products" fetch="LAZY">
      <join-columns>
        <join-column name="id_product" referenced-column-name="id_product"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
